{
  "compilerOptions": {
    "noImplicitAny": true,

    "noUnusedLocals": true,
    
    "noUnusedParameters": true,
    
    "sourceMap": true,
    
    "strictNullChecks": true,
   
    "target": "es2016",                                  
   
    "esModuleInterop": true,                        

    "forceConsistentCasingInFileNames": true,        

    "strict": true,                                 
    "skipLibCheck": true,
    "lib": [
      "ESNext",
      "DOM"
    ]
  }
}